@using Microsoft.AspNetCore.Identity

@model DFACore.Models.ApplicantsViewModel
@{
    ViewData["Title"] = "PartialApplicant";
    int increment = (int)ViewBag.Increment;
}
<style>
    #applicants-container input {
        text-transform: uppercase;
        font-weight: 500;
    }
</style>
<div dat-worth="@increment">
    @*<h1>@increment</h1>
        <label>Application Code: @ViewData["ApplicationCode" + increment]</label>*@
    @*<input asp-for="Records[increment].ApplicationCode" value="" class="form-control" />*@
    <div id="applicants-container" class="col-lg-12 col-md-12 col-sm-12" style="padding: 10px; border: 1px solid #a0a0a0; border-radius: 5px; margin-bottom: 25px;">
        @{ var documentOwnerNo = 1;
            documentOwnerNo += increment;
        }
        <h6 style="font-weight:bold">Document Owner @*no. <span>@documentOwnerNo</span>*@</h6>
        <div class="form-group">

            <div class="row">
                <div class="col-md-3 col-sm-12">
                    <label asp-for="Records[increment].FirstName">First Name <span class="red">*</span></label>
                    <input asp-for="Records[increment].FirstName" class="form-control fNamePartial" />
                    <span asp-validation-for="Records[increment].FirstName" class="text-danger fNamePartialValidation"></span>
                </div>

                <div class="col-md-3 col-sm-12">
                    <label asp-for="Records[increment].MiddleName"></label>
                    <input asp-for="Records[increment].MiddleName" class="form-control " />
                    <span asp-validation-for="Records[increment].MiddleName" class="text-danger "></span>
                </div>

                <div class="col-md-3 col-sm-12">
                    <label asp-for="Records[increment].LastName">Last Name <span class="red">*</span></label>
                    <input asp-for="Records[increment].LastName" class="form-control lNamePartial" />
                    <span asp-validation-for="Records[increment].LastName" class="text-danger lNamePartialValidation"></span>
                </div>


                <div class="col-md-3 col-sm-12">
                    <label asp-for="Records[increment].Suffix"></label>
                    @*<input asp-for="Suffix" class="form-control" />*@
                    <select asp-for="Records[increment].Suffix" class="form-control">
                        <option value="N/A">N/A</option>
                        <option value="JR.">JR.</option>
                        <option value="SR.">SR.</option>
                        <option value="I">I</option>
                        <option value="II">II</option>
                        <option value="III">III</option>
                        <option value="IV">IV</option>
                        <option value="V">V</option>
                        <option value="VI">VI</option>
                        <option value="VII">VII</option>
                        <option value="VIII">VIII</option>
                        <option value="IX">IX</option>
                        <option value="X">X</option>
                        <option value="XI">XI</option>
                        <option value="XII">XII</option>
                        <option value="XIII">XIII</option>
                        <option value="XIV">XIV</option>
                        <option value="XV">XV</option>
                    </select>
                    <span asp-validation-for="Records[increment].Suffix" class="text-danger"></span>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-md-6 col-sm-12">
                    <label asp-for="Records[increment].DateOfBirth">Date Birth <span class="red">*</span></label>
                    <input type="date" asp-for="Records[increment].DateOfBirth" class="datepicker form-control dateOfbirthPartial" />
                    <span asp-validation-for="Records[increment].CountryDestination" class="text-danger dateOfbirthPartialValidation"></span>
                </div>
                <div class="col-md-6 col-sm-12">
                    <label asp-for="Records[increment].CountryDestination">Country of Destination <span class="red">*</span></label>
                    <select asp-for="Records[increment].CountryDestination" class="form-control countrydestinationPartial">
                        <partial name="_CountriesPartial" />
                    </select>
                    <span asp-validation-for="Records[increment].CountryDestination" class="text-danger countrydestinationPartialValidation"></span>
                </div>
            </div>
        </div>
    </div>
    <div>
        <div class="col-12">

            @*</label>*@
        </div>
    </div>
    <div>
        <div>
            <a href="#" class="font-weight-bold button-other text-white" data-toggle="modal" data-target="#apostileModal@(increment)" id="documentsSelectionBtn@(increment)" style="font-size: 18px;padding: 8px;position: relative;top: 10px;">

                (+) ADD DOCUMENT/S
            </a>
            <span  class="text-danger apostiledataPartialValidation" style="position: relative;top: 20px;left: 10px;"></span>
            @*<h6>Authentication requirements may be subject to change. The Department may require additional supporting documents if necessary.</h6>*@
            <div style="background-color: #217ff3;
        color: white;
        height: auto;
        position: relative;top: 20px;
">
                <p class="text-left " style="padding: 8px;font-size:14px;">Number of documents to be Apostillized: <span style="font-weight:bold;" id="documentsCount@(increment)">0</span></p>
            </div>
            <div id="documentList" style="border: 1px solid #b7b7b7; padding: 10px;height:150px; overflow:scroll;position: relative;top: 10px;">

                <table class="table" id="documentsTable@(increment)">
                    <tr>
                        <th>Document</th>
                        <th>Quantity</th>
                        <th>Transaction</th>
                    </tr>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
        <div style="display:none;">
            <input asp-for="Records[increment].ApostileData" id="apostileData@(increment)" class="form-control apostiledataPartial" />

        </div>


    </div>
</div>

