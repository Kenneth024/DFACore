@using Microsoft.AspNetCore.Identity
@model DFACore.Models.ApplicantRecordViewModel
@{
    ViewData["Title"] = "Home Page";
}
<style>
    .nav-item a {
        height: 30px;
        line-height: 15px;
        border: 1px solid #b7b7b7;
        color: gray;
    }

    .nav-item .active {
        color: white;
    }

    div#application-form {
        color: gray;
    }

    /*button {
        width: 200px;
    }*/
    #documentList h6 {
        font-weight: 600;
    }

    #appointmentTime div {
        margin-left: 10px;
    }

    #step-one input {
        text-transform: uppercase;
        font-weight: 500;
    }
</style>

<div id="application-form" style="border: 1px solid #b7b7b7;
        padding: 20px;
        height: auto">
    <p style="margin-bottom: 2rem;">
        <span class="float-left font-weight-bold">
            @User.Identity.Name
        </span>
        <span class="float-right">
            <a asp-action="LogOff" asp-controller="Account" style="font-weight:bold;">Logout</a>
        </span>
        <span class="float-right" style="padding-right:20px;">
            @DateTime.Now.ToString("MMMM dd yyy HH:mm tt")
        </span>
    </p>
    <ul class="col-12 nav nav-pills nav-fill" style="margin-bottom: 1rem;">
        <li class="nav-item">
            <a id="stepOneBtn" class="nav-link" onclick="Navigate('step-one',this)" href="#step-one"><span class="font-weight-bold">STEP 1:</span><span> Apostille Application</span></a>
        </li>
        <li class="nav-item">
            <a id="stepTwoBtn" class="nav-link" onclick="Navigate('step-two',this)" href="#step-two"><span class="font-weight-bold">STEP 2:</span><span> Schedule an Appointment</span></a>
        </li>
        <li class="nav-item">
            <a id="stepThreeBtn" class="nav-link" onclick="Navigate('step-three',this)" href="#step-three"><span class="font-weight-bold">STEP 3:</span><span> Generate Appointment Letter</span></a>
        </li>
    </ul>
    <form method="post">
        <div id="step-one" class="tabcontent">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="form-group">
                    <h6>Name of the Document Owner</h6>
                    <div class="row">

                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="col-md-2 col-sm-12">
                            <label asp-for="Title"></label>
                            <select asp-for="Title" class="form-control">
                                <option value=""></option>
                                <option value="MR">MR.</option>
                                <option value="MRS">MRS.</option>
                                <option value="MRS">MS.</option>
                            </select>
                            <span asp-validation-for="Title" class="text-danger"></span>
                        </div>
                        <div class="col-md-3 col-sm-12">
                            <label asp-for="FirstName"></label>
                            <input asp-for="FirstName" class="form-control" />
                            <span asp-validation-for="FirstName" class="text-danger"></span>
                        </div>

                        <div class="col-md-3 col-sm-12">
                            <label asp-for="MiddleName"></label>
                            <input asp-for="MiddleName" class="form-control" />
                            <span asp-validation-for="MiddleName" class="text-danger"></span>
                        </div>

                        <div class="col-md-3 col-sm-12">
                            <label asp-for="LastName"></label>
                            <input asp-for="LastName" class="form-control" />
                            <span asp-validation-for="LastName" class="text-danger"></span>
                        </div>

                        <div class="col-md-1 col-sm-12">
                            <label asp-for="Suffix"></label>
                            @*<input asp-for="Suffix" class="form-control" />*@
                            <select asp-for="Suffix" class="form-control">
                                <option value=""></option>
                                <option value="JR.">JR.</option>
                                <option value="SR.">SR.</option>
                                <option value="I">I</option>
                                <option value="II">II</option>
                                <option value="III">III</option>
                                <option value="IV">IV</option>
                                <option value="V">V</option>
                            </select>
                            <span asp-validation-for="Suffix" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-6 col-sm-12">
                            <label asp-for="Address"></label>
                            <input asp-for="Address" class="form-control" />
                            <span asp-validation-for="Address" class="text-danger"></span>
                        </div>
                        <div class="col-md-3 col-sm-12">
                            <label asp-for="Nationality"></label>
                            <select asp-for="Nationality" class="form-control">
                                <option value=""></option>
                                <option value="FILIPINO">FILIPINO</option>
                            </select>
                            <span asp-validation-for="Nationality" class="text-danger"></span>
                        </div>

                        <div class="col-md-3 col-sm-12">
                            <label asp-for="ContactNumber"></label>
                            <input asp-for="ContactNumber" class="form-control" type="number" />
                            <span asp-validation-for="ContactNumber" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-8 col-sm-12">
                            <label asp-for="CompanyName"></label>
                            <input asp-for="CompanyName" class="form-control" />
                            <span asp-validation-for="CompanyName" class="text-danger"></span>
                        </div>
                        <div class="col-md-4 col-sm-12">
                            <label asp-for="CountryDestination"></label>

                            <select asp-for="CountryDestination" class="form-control">
                                <partial name="_CountriesPartial" />
                            </select>
                            <span asp-validation-for="CountryDestination" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-7 col-sm-12">
                            <label asp-for="NameOfRepresentative"></label>
                            <input asp-for="NameOfRepresentative" class="form-control" />
                            <span asp-validation-for="NameOfRepresentative" class="text-danger"></span>
                        </div>
                        <div class="col-md-5 col-sm-12">
                            <label asp-for="RepresentativeContactNumber"></label>
                            <input asp-for="RepresentativeContactNumber" class="form-control" type="number" />
                            <span asp-validation-for="RepresentativeContactNumber" class="text-danger"></span>
                        </div>
                    </div>
                </div>
            </div>


            <div class="form-group">
                <div class="col-12">
                    <label class="form-check-label">
                        <a href="#" class="font-weight-bold" data-toggle="modal" data-target="#apostileModal" id="documentsSelectionBtn">
                            Click here to select the document/s to be Apostillized
                        </a>
                    </label>
                </div>
            </div>
            <div class="form-group">
                <div class="col-12">
                    <h6>Authentication requirements may be subject to change. The Department may require additional supporting documents if necessary.</h6>
                    <div style="background-color: #217ff3;color: white;height: 35px;">
                        <p class="text-left " style="padding: 8px;font-size:14px;">Number of documents to be Apostillized: <span style="font-weight:bold;" id="documentsCount">0</span></p>
                    </div>
                    <div id="documentList" style="border: 1px solid #b7b7b7; padding: 10px;height:150px; overflow:scroll;">

                        <table class="table" id="documentsTable">
                            <tr>
                                <th>Quantity</th>
                                <th>Document</th>
                            </tr>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="col-12">
                    <input asp-for="ApostileData" id="apostileData" class="form-control" hidden />
                    <span asp-validation-for="ApostileData" class="text-danger"></span>
                </div>
                
            </div>
            <button id="stepOneNextBtn" type="button" class="btn col-md-4 col-sm-12" style="background-color:#217ff3;color:white;margin-left: 33.33%;margin-right: 33.33%;">Next</button>


            @*<button id="stepOneBackBtn" class="btn btn-secondary  col-md-4 col-sm-12" style="background: white;color: #217ff3;border: 1px solid darkgray;">Back</button>*@

        </div>
        <div id="step-two" style="display:none" class="tabcontent">
            <div class="row col-12">
                <label asp-for="ProcessingSite"></label>
                <input asp-for="ProcessingSite" id="site" class="form-control" value="DFA - Office of the Consular Affairs" />
            </div>
            <div class="row col-12">
                <label asp-for="ProcessingSiteAddress"></label>
                <input asp-for="ProcessingSiteAddress" id="address" class="form-control" value="Bradco Avenue, cor. Macapagal Blvd. ASEANA Business Park, Paranaque City" />
            </div>            <hr />
            <div class="row col-12" style="margin-top:10px;">
                <div class="col-md-8 col-sm-12">
                    <div style="border:1px solid #b7b7b7;padding: 10px;">
                        <label>Select Available Schedule</label>
                        @*<input class="form-control" type="date" />*@
                        @*<div id="datetimepicker"></div>*@
                        @*@Html.Partial("_CalendarPartial")*@
                        <partial name="_CalendarPartial" />
                    </div>
                </div>
                @*<div class="col-md-2 col-sm-12">
                        <label>Select Time</label>
                        <div id="appointmentTime" style="border:1px solid #b7b7b7;height:auto;margin-left:10px;">
                            <div class="col-12">
                                <input type="radio" name="option1" />
                                <label for="option1">8:00 AM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option2" />
                                <label for="option2">9:00 AM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option3" />
                                <label for="option3">10:00 AM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option4" />
                                <label for="option4">11:00 AM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option5" />
                                <label for="option5">12:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option6" />
                                <label for="option6">1:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option7" />
                                <label for="option7">2:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option8" />
                                <label for="option8">3:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option9" />
                                <label for="option9">4:00 PM</label>
                            </div>
                        </div>
                    </div>*@
                <div class="col-md-4 col-sm-12">
                    <div style="border:1px solid #b7b7b7;height:auto;padding:10px;">
                        <p>Selected Date and Time</p>
                        @*<p class="font-weight-bold">May 20, 2020 Friday 11:00 AM</p>*@
                        <input asp-for="ScheduleDate" id="appointmentSchedule" class="form-control font-weight-bold">
                        <span asp-validation-for="ScheduleDate" class="text-danger"></span>
                        <p id="availableSchedMessage" class="text-success font-weight-bold" style="margin-top:10px;display:none;">Schedule available</p>
                        <p id="unAvailableSchedMessage" class="text-danger font-weight-bold" style="margin-top:10px;display:none;">Schedule unavailable</p>
                        <p style="margin-top:10px;">Processing Site</p>
                        @*<p class="font-weight-bold">(Site and Address)</p>*@
                        <p id="siteAndAddress" class="font-weight-bold"></p>
                        <div>
                            <button type="button" class="btn btn-secondary  col-md-4 col-sm-12 backBtn" style="background: white;color: blue;border: 1px solid darkgray;">Back</button>
                            <button id="nextBtn" class="btn col-md-4 col-sm-12 float-right" style="background-color:#217ff3;color:white;">Confirm</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>

    <!-- Modal -->
    <div class="modal fade" id="apostileModal" tabindex="-1" role="dialog" aria-labelledby="apostileModallLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Select Documents</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <table class="table table-bordered">
                        <thead></thead>
                        <tr style="background-color: #217ff3;color: white;height: 35px;">
                            <th>TYPE OF DOCUMENT</th>
                            <th>GUIDELINES</th>
                        </tr>
                        <tbody>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label font-weight-bold">
                                            <input type="checkbox" class="form-check-input" id="nbiClearance" value="NBI Clearance/Sundry">1. NBI Clearance/Sundry
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Original document issued by National Bureau of Investigation (NBI) with dry seal.</p>
                                    <p>*Personal Copy is NOT valid.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <label class="form-check-label font-weight-bold">
                                        2. PSA/NSO/Local Civil Registrar
                                    </label>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="birthCertificate" value="Birth Certificate"> Birth Certificate
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="marriageCertificate" value="Marriage Certificate"> Marriage Certificate
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="deathCertificate" value="Death Certificate"> Death Certificate
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="cenomar" value="Certificate of No Marriage Record"> Certificate of No Marriage Record (CENOMAR, Advisory on Marriage and/or Negative Records)
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Original document issued by Philippine Statistics Authority (PSA)/ National Statistics Office (NSO).</p>
                                    <p>*For newly registered records, Local Civil Registrar (LCR) copy should be certified by PSA.</p>
                                    <p>*Provide an LCR copy of Birth (Form 1A)/ Death (Form 2A)/ Marriage (Form 3A) Certificate if the entries from PSA/NSO are UNCLEAR.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <label class="form-check-label font-weight-bold">
                                        3. School Documents
                                    </label>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="elementaryAndHighschool" value="Elementary and High School Level (Form 137 and Diploma)"> For Elementary and High School Level (Form 137 and Diploma)
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Certified True Copies from school; and</p>
                                    <p>*Certification, Authentication and Verification (CAV) from DepEd Regional Office.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="techVoch" value="Technical and Vocational Courses (TOR and Diploma/National Certificate)"> For Technical and Vocational Courses (TOR and Diploma/National Certificate)
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Certified True Copies from school; and</p>
                                    <p>*Certification, Authentication and Verification (CAV) from Technical and Skills Development Authority (TESDA).</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="stateCollegesAndUniversities" value="State Colleges and Universities (TOR and Diploma)"> For State Colleges and Universities (TOR and Diploma)
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Certified True Copies from school; and Certification, Authentication and Verification (CAV) from the school.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="privateOrLocalColleges" value="Private/Local Colleges and Universities (TOR and Diploma)"> For Private/Local Colleges and Universities (TOR and Diploma)
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Certified True Copies from school; and </p>
                                    <p>*Certification, Authentication and Verification (CAV) from Commission on Higher Education (CHED). </p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label font-weight-bold">
                                            <input type="checkbox" class="form-check-input" id="PRC" value="Professional Regulation Commission (PRC) document/s"> 4. Professional Regulation Commission (PRC) document/s
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <input type="number" class="small" id="qtyPRC" style="width: 15%;height:30px;">
                                        <span> Please insert no. of document/s (if more than one (1) type of document)</span>
                                        @*<label for="qtyPRC"></label>*@
                                    </div>
                                </td>
                                <td>
                                    <p>*Certified True Copies from PRC.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label font-weight-bold">
                                            <input type="checkbox" class="form-check-input" id="medCerts" value="Medical Certificates"> 5. Medical Certificates
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <input type="number" class="small" id="qtyMedCerts" style="width: 15%;height:30px;">
                                        <span> Please insert no. of document/s (if more than one (1) type of document)</span>
                                        @*<label for="qtyPRC"></label>*@
                                    </div>
                                </td>
                                <td>
                                    <p>*For employement: DOH Stamp per document.</p>
                                    <p>*For other purpose: Certification issued by the DOH with attached Medical Certificate.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label font-weight-bold">
                                            <input type="checkbox" class="form-check-input" id="CAAP" value="Civil Aviation Authority of the Philippines (CAAP) issued document/s"> 6. Civil Aviation Authority of the Philippines (CAAP) issued document/s
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <input type="number" class="small" id="qtyCAAP" style="width: 15%;height:30px;">
                                        <span> Please insert no. of document/s (if more than one (1) type of document)</span>
                                        @*<label for="qtyPRC"></label>*@
                                    </div>
                                </td>
                                <td>
                                    <p>*Certificate by CAAP.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-check">
                                        <label class="form-check-label font-weight-bold">
                                            <input type="checkbox" class="form-check-input" id="driverLicense" value="Driver's License"> 7. Driver's License
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Certification from Land Transportation Office (LTO Main Branch Only).</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <label class="form-check-label font-weight-bold">
                                        8. Documents issued by a private entity
                                    </label>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="COE" value="Certificate of Employment"> Certificate of Employment
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="invitationLetter" value="Invitation Letter"> Invitation Letter
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="trainings" value="Trainings"> Trainings
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="baptismalCert" value="Baptismal Certificate"> Baptismal Certificate
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="privateIssuedOtherDocu" value="Other Document"> Other Document
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Notarized Affidavit stating financial circumstances and indicating certificate/s as attachments.</p>
                                    <p>*Certificate of Authority for a Notarial Act (CANA) signed by the Executive Judge/Vice-Executive Judge/any office authorized signatories (issued by the Regional Trial Court).</p>
                                    <p>*Copy of Notarial Commission is not the same as Certificate of Authority for a Notarial Act.</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <label class="form-check-label font-weight-bold">
                                        9. Notary Public Documents
                                    </label>
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input type="checkbox" class="form-check-input" id="specialPowerOfAttorney" value="Special Power of Attorney"> Special Power of Attorney
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="MOA" value="Memorandum of Agreement"> Memorandum of Agreement
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="MOU" value="Memorandum of Understanding"> Memorandum of Understanding
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="anyContract" value="Any Form of Contract"> Any Form of Contract
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="affidavitOfConsent" value="Affidavit of Consent or Advise"> Affidavit of Consent or Advise
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="jointAffidavit" value="Joint Affidavit"> Joint Affidavit
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="otherAffidavit" value="Other Affidavit"> Other Affidavit
                                        </label>
                                    </div>
                                </td>
                                <td>
                                    <p>*Certificate of Authority for a Notarial Act (CANA) signed by the Executive Judge/Vice-Executive Judge/any office authorized signatories (issued by the Regional Trial Court).</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <label class="form-check-label font-weight-bold">
                                        10. Court Document/s
                                    </label>
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input type="checkbox" class="form-check-input" id="courtDecision" value="Court Decision"> Decision
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input type="checkbox" class="form-check-input" id="courtResolution" value="Court Resolution/Order"> Resolution/Order
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <label class="form-check-label ">
                                            <input type="checkbox" class="form-check-input" id="courtOtherDocument" value="Court Other Document"> Other Document
                                        </label>
