                            <div class="col-12">
                                <input type="radio" name="option4" />
                                <label for="option4">11:00 AM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option5" />
                                <label for="option5">12:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option6" />
                                <label for="option6">1:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option7" />
                                <label for="option7">2:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option8" />
                                <label for="option8">3:00 PM</label>
                            </div>
                            <div class="col-12">
                                <input type="radio" name="option9" />
                                <label for="option9">4:00 PM</label>
                            </div>
                        </div>
                    </div>*@
                <div class="col-md-4 col-sm-12">
                    <div style="border:1px solid #b7b7b7;height:auto;padding:10px;">
                        <p>Selected Date and Time</p>
                        @*<p class="font-weight-bold">May 20, 2020 Friday 11:00 AM</p>*@
                        <input asp-for="ScheduleDate" id="appointmentSchedule" class="form-control font-weight-bold" disabled>
                        <p style="margin-top:10px;">Processing Site</p>
                        @*<p class="font-weight-bold">(Site and Address)</p>*@
                        <p id="siteAndAddress" class="font-weight-bold"></p>
                        <div>
                            <button id="nextBtn" class="btn col-md-4 col-sm-12" style="background-color:#217ff3;color:white;">Confirm</button>
                            <button class="btn btn-secondary float-right col-md-4 col-sm-12" style="background: white;color: blue;border: 1px solid darkgray;">@Html.ActionLink("Back", "Index")</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div id="step-three" style="display:none" class="tabcontent">
        <h6 class="font-weight-bold">Application Summary</h6>
        <div class="row col-md-12">
            <div class="col-md-4 col-sm-12">
                <span>Name of Applicant </span>
            </div>
            <div class="col-md-8 col-sm-12">
                <span>: Renz Roy Gososo</span>
            </div>
            <div class="col-md-4 col-sm-12">
                <span>Type of Document to be Apostillized </span>
            </div>
            <div class="col-md-8 col-sm-12">
