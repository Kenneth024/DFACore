@section Scripts {
    <script>
        $(document).ready(function () {

            $('#stepOneBtn').click();
            $('#siteAndAddress').text($('#site').val().concat(' ', $('#address').val()));

            $('.backBtn').click(function () {
                $('#stepOneBtn').click();
            });
            $('#stepOneNextBtn').click(function () {
                $('#stepTwoBtn').click();
            });
            $('#stepThreeBackBtn').click(function () {
                $('#stepTwoBtn').click();
                //$('#stepOneBtnContainer').hide();
            });
            $('#documentsCount').text(0);
            $('#documentsSelectionBtn').click(function () {
                //$("#documentsTable tbody").empty();
            });

            $('#selectDocumentsBtn').click(function () {
                var documentObject = [];
                $(':checkbox:checked').each(function (i) {
                    if ($(this).val() == "Professional Regulation Commission (PRC) document/s") {
                        documentObject[i] = ({ Name: $(this).val(), Quantity: !$('#qtyPRC').val() ? 1 : parseInt($('#qtyPRC').val()) });
                    }
                    else if ($(this).val() == "Medical Certificates") {
                        documentObject[i] = ({ Name: $(this).val(), Quantity: !$('#qtyMedCerts').val() ? 1 : parseInt($('#qtyMedCerts').val()) });
                    }
                    else if ($(this).val() == "Civil Aviation Authority of the Philippines (CAAP) issued document/s") {
                        documentObject[i] = ({ 'Name': $(this).val(), 'Quantity': !$('#qtyCAAP').val() ? 1 : parseInt($('#qtyCAAP').val()) });
                    }
                    else if ($(this).val() == "Immigration Record/s") {
                        documentObject[i] = ({ 'Name': $(this).val(), 'Quantity': !$('#qtyImmigration').val() ? 1 : parseInt($('#qtyImmigration').val()) });
                    }
                    else if ($(this).val() == "DSWD Clearance") {
                        documentObject[i] = ({ 'Name': $(this).val(), 'Quantity': !$('#qtyDSWDClearance').val() ? 1 : parseInt($('#qtyDSWDClearance').val()) });
                    }
                    else if ($(this).val() == "Business Registration & Other Documents issued by a Government Agency") {
                        documentObject[i] = ({ 'Name': $(this).val(), 'Quantity': !$('#qtyBusinessRegistration').val() ? 1 : parseInt($('#qtyBusinessRegistration').val()) });
                    }
                    else if ($(this).val() == "Export Document/s") {
                        documentObject[i] = ({ 'Name': $(this).val(), 'Quantity': !$('#qtyExportDocuments').val() ? 1 : parseInt($('#qtyExportDocuments').val()) });
                    }
                    else {
                        documentObject[i] = ({ Name: $(this).val(), Quantity: 1 });
                    }
                });
                $("#documentsTable tbody > tr").empty();
                $("#documentsTable").append('<tr><th>Quantity</th><th>Document</th></tr>');
                //var tbodyContent = '';
                $.each(documentObject, function (index, value) {
                    $('#documentsTable tr:last').after('<tr><td>'.concat(value.Quantity, '</td><td>', value.Name, '</td></tr>'));
                    //tbodyContent += '<tr><td>'.concat(value.Quantity, '</td><td>', value.Name, '</td></tr>');
                });
                //console.log(tbodyContent);
                //$('#documentsTable tbody').append(tbodyContent);
                $('#documentsCount').text(documentObject.length);
                $('#apostileData').val(JSON.stringify(documentObject));
                console.log(JSON.stringify(documentObject));
            });

        });


        function Navigate(step, element) {
            var i, tabcontent, tablinks;
            tabcontent = $('.tabcontent');
            for (i = 0; i < tabcontent.length; i++) {
                tabcontent[i].style.display = "none";
            }
            tablinks = $('.nav-link');
            for (i = 0; i < tablinks.length; i++) {
                tablinks[i].style.backgroundColor = "";
                tablinks[i].style.color = "gray";
            }
            element.style.backgroundColor = "#217ff3";
            element.style.color = "white";
            document.getElementById(step).style.display = "block";
        }

    </script>
}
