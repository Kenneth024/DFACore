                    .append($("<label>Middle Name</label>").attr("asp-for", "Records[" + i + "].MiddleName"))
                    .append($("<input />").attr("class", "form-control").attr("name", "Records[" + i + "].MiddleName"));
                var lName = $("<div />").attr("class", "col-md-3 col-sm-12")
                    .append($("<label>Last Name <span class='red'>*</span></label>").attr("asp-for", "Records[" + i + "].LastName"))
                    .append($("<input />").attr("class", "form-control").attr("name", "Records[" + i + "].LastName"));
                var suffix = $("<div />").attr("class", "col-md-3 col-sm-12")
                    .append($("<label>Suffix</label>").attr("asp-for", "Records[" + i + "].Suffix"))
                    .append($("<select />").attr("class", "form-control").attr("name", "Records[" + i + "].Suffix"));

                var dateOfBirth = $("<div />").attr("class", "col-md-6 col-sm-12").append($("<input />").attr("class", "form-control").attr("name", "Records[" + i + "].DateOfBirth"));

                var formGroup1 = $("<div />").attr("class", "form-group");
                var formGroup2 = $("<div />").attr("class", "form-group");
                //<a href="C:\DFA\DFACore\Views\Shared\_CountriesPartial.cshtml">C:\DFA\DFACore\Views\Shared\_CountriesPartial.cshtml</a>
                var countryDest = $("<div />").attr("class", "col-md-6 col-sm-12").append($("<select />").attr("class", "form-control").attr("name", "Records[" + i + "].CountryDestination").load("countries.html"));

                var rowNameDiv = $("<div />").attr("class", "row");
                var rowDOBDiv = $("<div />").attr("class", "row");
                var colLg12Div = $("<div />").attr("class", "col-sm-12"); //<div class="col-md-3 col-sm-12">
                var md2Div = $("<div />").attr("class", "col-md-2 col-sm-12");

                rowNameDiv.append(fName);
                rowNameDiv.append(mName);
                rowNameDiv.append(lName);
                rowNameDiv.append(suffix);
                
                rowDOBDiv.append(dateOfBirth);
                rowDOBDiv.append(countryDest);

                formGroup1.append(rowNameDiv);
                formGroup2.append(rowDOBDiv);

                colLg12Div.append(formGroup1);
                colLg12Div.append(formGroup2);

                div.append(colLg12Div);

                var removeButton = $("<input />").attr("type", "button").attr("value", "Remove");
                removeButton.attr("onclick", "RemoveApplicant(this)");
                div.append(removeButton);
                $("#ApplicantContainer").append(div);
